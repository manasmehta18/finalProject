{
  "name": "final-project",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://github.com/Oporto/fp"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "scripts": {
    "start": "node server.js",
    "parcel": "parcel src/html/main.html --public-url='./'",
    "watch": "parcel watch src/html/*.html --public-url='./'",
    "build": "parcel build src/html/*.html --public-url='./'",
    "css": "npx tailwind build src/css/main-t.css -o src/css/main-styles.css; npx tailwind build src/css/login-t.css -o src/css/login-styles.css; npx tailwind build src/css/profile-t.css -o src/css/profile-styles.css;"
  },
  "proxy": "http://localhost:3000/",
  "dependencies": {
    "@babel/preset-env": "^7.6.3",
    "@material-ui/core": "^4.5.0",
    "aos": "^2.3.4",
    "babelify": "^10.0.0",
    "body-parser": "^1.19.0",
    "browserify": "^16.5.0",
    "bulma": "^0.7.5",
    "compression": "^1.7.4",
    "dat.gui": "^0.7.6",
    "easy-peasy": "^3.1.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.6.0",
    "firestore": "^1.1.6",
    "helmet": "^3.21.1",
    "parcel": "^1.12.4",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "serve-favicon": "^2.5.0",
    "show-toast": "^1.1.4",
    "tailwindcss": "^1.1.2",
    "vis": "^4.21.0-EOL"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "babel-preset-nano-react-app": "^0.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "nano-react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  }
}
